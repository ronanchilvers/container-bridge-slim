{
    "name": "ronanchilvers/container-bridge-slim",
    "description": "Convenience bridge for using ronanchilvers/container with slim3",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronan Chilvers",
            "email": "ronan@thelittledot.com"
        }
    ],
    "require": {
        "slim/slim": "^3.9",
        "ronanchilvers/container": "^1.0"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ronanchilvers\\Container\\Slim\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ronanchilvers\\Container\\Slim\\Test\\": "tests"
        }
    }
}
